{
    "configurations": [
        {
            "name": "Debug Qt Application with cppdbg",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            //"visualizerFile": "${command:qt-official.natvis}",
            "showDisplayString": true,
            "windows": {
                "environment": [
                    {
                        "name": "PATH",
                        "value": "${env:PATH};${command:qt-official.kitDirectory}\\bin"
                    },
                    {
                        "name": "CMAKE_PREFIX_PATH",
                        "value": "F:/QT/6.7.2/mingw_64"
                    }
                ],
                "MIMode": "gdb",
                "miDebuggerPath": "${command:qt-official.minGWgdb}"
            },
            "environment": []
        },
        {
            "name": "Debug Qt Application with Visual Studio Debugger",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            //"visualizerFile": "${command:qt-official.natvis}",
            "windows": {
                "environment": [
                    {
                        "name": "PATH",
                        "value": "${env:PATH};${command:qt-official.kitDirectory}\\bin"
                    },
                    {
                        "name": "CMAKE_PREFIX_PATH",
                        "value": "${command:qt-official.kitDirectory}"
                    },
                    {
                        "name": "QxOrm_dir",
                        "value": "F:/Desktop/QxOrm-1.4.9"
                    }
                ]
            }
        }
    ]
}